package in.co.rays.jdbc.preparedstatement;

import java.util.Date;

public class TestUserModel 
{
	public static void main(String[] args) throws Exception 
	{
		UserBean bean = new UserBean();
		
		bean.setId(1);
		bean.setFirstName("Apurva");
		bean.setLastName("Deshmukh");
		bean.setLogin("apurva@gmail.com");
		bean.setPassword("pass1234");
		bean.setDob(new Date());
		
		UserModel model = new UserModel();
		
		int id = model.add(bean);
		
		System.out.println("Record inserted at id: " + id);
	}
}
